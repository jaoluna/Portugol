algoritmo "contadorcomsalto"
var
    salto, ini, fim: inteiro
inicio
      escreva("Digite o primeiro valor: ")
      leia(ini)
      escreva("Digite o ultimo valor: ")
      leia(fim)
      escreva("Qual sera o incremento? ")
      leia(salto)
      enquanto (ini<=fim) faca
               escreva(ini)
               ini<-ini+salto
      fimenquanto
fimalgoritmo