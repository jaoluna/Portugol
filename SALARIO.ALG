algoritmo "Salario"
var
   nome: caractere
   salario: real
inicio
      Escreva("Escreva o seu nome:")
      Leia(nome)
      Escreva("Escreva o valor do seu salario")
      Leia(salario)
      Escreva(" O funcionario ", nome, " tem uma salario de ", salario, " este mes")
      

fimalgoritmo