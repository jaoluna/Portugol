algoritmo "contarateondequer"
var
   contador, num: inteiro
inicio
      escreva("Quer contar ate quanto?")
      leia(num)
      contador<-0
      enquanto (contador<=num) faca
               escreval(contador)
               contador<- contador+1
      fimenquanto
      escreval("Acabei de contar")
fimalgoritmo