algoritmo "media"
var
   nota1, nota2, media: real
inicio
      escreva("informe a primeira nota: ")
      leia(nota1)
      escreva("informe a segunda nota: ")
      leia(nota2)
      media <- (nota1+nota2)/2
      escreva("a media de ", nota1, " e ", nota2, " e igual a", media)
fimalgoritmo