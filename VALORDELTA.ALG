algoritmo "valorDelta"
var
   A, B, C, delta: real
inicio
      escreva("Insira o valor de A: ")
      leia(A)
      escreva("Insira o valor de B: ")
      leia(B)
      escreva("Insira o valor de C: ")
      leia(C)
      delta <- B^2 - 4*A*C
      escreva("O valor de delta e: ", delta)
fimalgoritmo