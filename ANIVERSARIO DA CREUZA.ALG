algoritmo "aniversario da creuza"

var
    idade, ano_atual, ano_nascimento: inteiro
inicio
      Escreva("Em que ano estamos?")
      Leia(ano_atual)
      Escreva("Qual seu ano de nascimento?")
      Leia(ano_nascimento)
      idade <- ano_atual - ano_nascimento
      Escreva("Sua idade e ", idade)

fimalgoritmo